{
    "manifest_version": 3,
    "name": "Note アカウント切替君",
    "version": "1.2",
    "permissions": [
        "cookies",
        "storage",
        "tabs"
    ],
    "host_permissions": [
        "https://note.com/*"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": "icon.png"
    }
}